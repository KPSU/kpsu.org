<html>
<head>
  <%= render :partial => 'layouts/meta' %>

  <title>KPSU | <%= yield(:title) %></title>
  <%= csrf_meta_tag %>
  <%= stylesheet_link_tag "production.alt.css", :cache => true %>
  <%= javascript_include_tag 'production.alt.js', :cache => true %>
	
  <% if RAILS_ENV == "development" %>
